<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.controlecolesterol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ControleColesterol">
        <activity android:name=".UserPreferences"></activity>
        <activity android:name=".ListaDeMedicamentosActivity" />
        <activity android:name=".AutoriaActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".UsuarioActivity" />
        <activity
            android:name=".AlimentosActivity"
            android:parentActivityName=".ListaDeAlimentosActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".ListaDeAlimentosActivity" />

        <activity android:name=".MedicamentosActivity" />
        <activity android:name=".ListaDeAlimentosActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>